<!--pages/shop/shop.wxml-->
<view class="container">

  <view class="doc-title zan-hairline--bottom">PHOTO</view>

  <view class="zan-panel">
    <view wx:for="{{ pictures }}" wx:key="unique" class="zj-wrap">
      <image class="zj-img" src="{{item}}" data-index="{{index}}" bindtap="previewImage"></image>
    </view>
  </view>
</view>


